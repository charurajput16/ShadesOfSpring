    {
      "scope": ["markup.italic.markdown", "punctuation.definition.italic.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C792EA"
      }
    },
    {
      "scope": ["markup.inline.raw.string.markdown", "markup.fenced_code.block.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#0460b1"
      }
